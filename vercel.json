{
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python@2.10.0",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.12",
        "includeFiles": [
          "webapp/**/*",
          "*.py"
        ]
      }
    },
    {
      "src": "webapp/static/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/webapp/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.py"
    }
  ]
}
